{
  "name": "kjos/orchestra",
  "version": "1.3.3",
  "description": "Orchestra tool for Laravel and other",
  "license": "MIT",
  "authors": [
    {
      "name": "Koffi Konan Kan Jean Sylvain",
      "email": "jeankoffi543@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/jeankoffi543/orchestra.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 ||^12.0",
    "illuminate/database": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "illuminate/routing": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
    "laravel/pint": "^1.20",
    "archtechx/enums": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Kjos\\Orchestra\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Kjos\\Orchestra\\Tests\\": "tests/",
      "App\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kjos\\Orchestra\\OrchestraServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "format": [
      "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    ],
    "analyse": [
      "vendor/bin/phpstan analyse --memory-limit=1G"
    ]
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "nunomaduro/collision": "^5.0",
    "fakerphp/faker": "^1.23",
    "phpro/grumphp": "^2.5",
    "mockery/mockery": "^1.6",
    "pestphp/pest": "^1.23",
    "orchestra/testbench": "^6.47",
    "pestphp/pest-plugin": "^1.1",
    "pestphp/pest-plugin-laravel": "^1.4",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.75"
  }
}
